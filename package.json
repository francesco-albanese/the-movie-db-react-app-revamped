  "private": true,
  "description": "Progressive web app powered by React, Redux and Material UI üí™üèªüòé",
  "dependencies": {
    "axios": "^0.18.0",
    "classcat": "^3.2.3",
    "lodash-es": "^4.17.11",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@material-ui/core": "^3.1.2",
    "@material-ui/icons": "^3.0.1",
    "autoprefixer": "^9.1.5",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.18",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react-app": "^5.0.1",
    "babel-runtime": "^6.26.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "chalk": "1.1.3",
    "css-loader": "0.28.7",
    "dotenv": "^6.0.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-react-app": "^3.0.2",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "^2.0.0",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.1.1",
    "jest": "^23.6.0",
    "node-sass": "^4.9.3",
    "node-sass-glob-importer": "^5.2.0",
    "object-assign": "^4.1.1",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "promise": "^8.0.2",
    "prop-types": "^15.6.2",
    "raf": "3.4.0",
    "react-dev-utils": "^6.0.1",
    "react-loadable": "^5.5.0",
    "react-test-renderer": "^16.5.2",
    "resolve": "^1.8.1",
    "sass-loader": "^7.1.0",
    "scss-loader": "0.0.1",
    "style-loader": "^0.23.0",
    "stylelint": "^9.6.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^1.0.0",
    "stylelint-scss": "^3.3.1",
    "sw-precache-webpack-plugin": "^0.11.5",
    "url-loader": "0.6.2",
    "webpack": "^4.20.2",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.9",
    "webpack-manifest-plugin": "^2.0.4",
    "whatwg-fetch": "2.0.3"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run stylelint:fix && npm run eslint:fix && npm run test:hook"
    }
  },
  "scripts": {
    "start": "node config/scripts/start.js",
    "build": "node config/scripts/build.js",
    "eslint": "eslint --max-warnings 0 ./src",
    "eslint:fix": "npm run eslint -- --fix",
    "test": "node config/scripts/test.js --env=jsdom",
    "test:hook": "npm test -- --forceExit",
    "stylelint": "stylelint \"./src/**/*.scss\" --config \"config/stylelint/stylelintrc.js\"",
    "stylelint:fix": "npm run stylelint -- --fix",
    "prepush-hook": "npm run stylelint:fix && npm run eslint:fix && npm run test:hook"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/jest/polyfills.js",
      "<rootDir>/config/jest/enzyme-adapter.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "#(.*)$": "<rootDir>/src/$1"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
